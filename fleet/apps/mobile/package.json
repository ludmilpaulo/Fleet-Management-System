    "submit:production": "bash scripts/submit-production.sh all",
    "build-and-submit:ios": "npm run build:ios && npm run submit:ios",
    "build-and-submit:android": "npm run build:android && npm run submit:android",
    "build-and-submit:all": "npm run build:production && npm run submit:production"
